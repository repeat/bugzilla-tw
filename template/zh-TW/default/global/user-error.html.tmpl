[%# INTERFACE:
  # header_done: boolean. True if the Bugzilla header has already been printed.
  # error: string. The tag of the error, or the error message to be displayed
  # (deprecated). May contain HTML if it's an error message.
  #%]

[%# This is a list of all the possible user errors. Please keep them in
  # alphabetical order by error tag, and leave a blank line between errors.
  #
  # Note that you must explicitly filter every single template variable
  # in this file; if you do not wish to change it, use the "none" filter.
  #
  # Extension- or custom-specific error handling  can be easily added
  # via hooks: just place additional code into
  # template/en/hook/global/user-error-errors.html.tmpl
  # Note: be aware of uniqueness of error string parameter value, since
  # nobody can guarantee the hook files processing order in the future.
  #%]

[% PROCESS global/variables.none.tmpl %]

[% PROCESS "global/field-descs.none.tmpl" %]

[% DEFAULT title = "錯誤" %]

[% error_message = BLOCK %]
  [% IF    error == "account_creation_disabled" %]
    [% title = "不允許建立帳號" %]
    不允許建立帳號。
    <hr>
    新的帳號必須由管理者 ([% Param("maintainer") %]) 建立。

  [% ELSIF error == "account_creation_restricted" %]
    [% title = "帳號建立已被限制" %]
    建立使用者帳號的行為已被限制。
    <hr>
    要建立帳號，請洽管理者 ([% Param("maintainer") %]) 。

  [% ELSIF error == "account_disabled" %]
    [% title = "帳號已被停權" %]
    [% disabled_reason FILTER none %]
    <hr>
    如果你認為你不應被停權，請寄信給 [% Param("maintainer") %] 並解釋原因。

  [% ELSIF error == "account_exists" %]
    [% title = "帳號已存在" %]
    帳號名稱[% IF email %] [% email FILTER html %] [% END %]已存在。

  [% ELSIF error == "account_locked" %]
    [% title = "帳號已鎖住" %]
    因為你已超過嘗試登入的次數，
    你的 IP ([% ip_addr FILTER html %]) 在
    [% unlock_at FILTER time %] 前已不被允許登入此帳號。

  [% ELSIF error == "alias_has_comma_or_space" %]
    [% title = "別名中有無效的字元" %]
    你所輸入的別名 <em>[% alias FILTER html %]</em>
    內包含了至少一個逗號 (,) 或空白。由於逗號及空白是用來分隔在清單中的每一個別名，
    因此無法在別名中使用它們。請選擇不包含逗號和空白的別名。

  [% ELSIF error == "alias_in_use" %]
    [% title = "別名使用中" %]
    [% terms.Bug %] [% bug_id FILTER bug_link(bug_id) FILTER none %]
    已使用別名 <em>[% alias FILTER html %]</em> 。
    請選擇另一個別名。

  [% ELSIF error == "alias_is_numeric" %]
    [% title = "別名不能全為數字" %]
    你試著替 [% terms.bug %] 取這個別名 <em>[% alias FILTER html %]</em> ，
    但是別名不能全為數字，以免和
    [% terms.bug %] 編號搞混。請選擇至少有一個英文字母的別名。

  [% ELSIF error == "alias_too_long" %]
    [% title = "別名太長" %]
    [% terms.Bug %]別名不能超過 20 字元。
    請選擇較短的別名。

  [% ELSIF error == "auth_cant_create_account" %]
    [% title = "無法建立帳號" %]
    如要建立帳號，請洽管理者。

  [% ELSIF error == "auth_failure" %]
    [% title = "需要授權" %]
    [% admindocslinks = {'groups.html' => '群組安全性'} %]
    很抱歉，
    [% IF group %]
      你並非 '[% group FILTER html %]' 群組的成員，
    [% END %]

    [% IF reason %]
      [% IF group %]而且[% END %]
      [% IF reason == "cant_bless" %]
        你也沒有在群組新增或刪除成員的權限，
      [% ELSIF reason == "not_visible" %]
        對特定的使用者群組有瀏覽的權限限制，
      [% END %]
    [% END %]

    [% IF group || reason %]而且[% END %]你並沒有權限可以
    [% IF action == "access" %]
      進入
    [% ELSIF action == "add" %]
      新增
    [% ELSIF action == "begin" %]
      開始
    [% ELSIF action == "modify" %]
      修改
    [% ELSIF action == "delete" %]
      刪除
    [% ELSIF action == "edit" %]
      新增、修改或刪除
    [% ELSIF action == "move" %]
      搬移
    [% ELSIF action == "run" %]
      執行
    [% ELSIF action == "schedule" %]
      排程
    [% ELSIF action == "use" %]
      使用
    [% ELSIF action == "approve" %]
      允許
    [% END %]

    [% IF object == "administrative_pages" %]
      管理用頁面
    [% ELSIF object == "attachment" %]
    [% IF attach_id %]
        附件 #[% attach_id FILTER html %]
      [% ELSE %]
        此附件
      [% END %]
    [% ELSIF object == "bugs" %]
      [%+ terms.bugs %]
    [% ELSIF object == "charts" %]
      「建立新圖表」的功能
    [% ELSIF object == "classifications" %]
      分類
    [% ELSIF object == "components" %]
      子產品
    [% ELSIF object == "custom_fields" %]
      自訂欄位
    [% ELSIF object == "field_values" %]
      欄位值
    [% ELSIF object == "flagtypes" %]
      旗標類型
    [% ELSIF object == "group_access" %]
      瀏覽群組
    [% ELSIF object == "groups" %]
      群組
    [% ELSIF object == "keywords" %]
      關鍵字
    [% ELSIF object == "milestones" %]
      里程碑
    [% ELSIF object == "multiple_bugs" %]
      數個[% terms.bugs %]
    [% ELSIF object == "parameters" %]
      參數
    [% ELSIF object == "products" %]
      產品
    [% ELSIF object == "quips" %]
      智語
    [% ELSIF object == "reports" %]
      訂閱報告
    [% ELSIF object == "sanity_check" %]
      健全度測試
    [% ELSIF object == "settings" %]
      設定
    [% ELSIF object == "sudo_session" %]
      執行 sudo
    [% ELSIF object == "timetracking_summaries" %]
      以時間進行追蹤的摘要報告
    [% ELSIF object == "user" %]
      [% IF userid %]
      使用者帳號 '[% userid FILTER html %]'
      [% ELSE %]
      你指定的使用者
      [% END %]
    [% ELSIF object == "users" %]
      使用者
    [% ELSIF object == "versions" %]
      版本
    [% ELSIF object == "workflow" %]
      流程
    [% END %]。

    [% Hook.process("auth_failure") %]

  [% ELSIF error == "attachment_deletion_disabled" %]
    [% title = "禁止刪除附件" %]
    已禁止刪除附件。

  [% ELSIF error == "attachment_illegal_url" %]
    [% title = "不合法的附件網址" %]
    <em>[% url FILTER html %]</em> 並非合法的附件網址。
    網址必須以 http:// 、 https:// 或 ftp:// 開頭。

  [% ELSIF error == "attachment_removed" %]
    [% title = "附件已移除" %]
    你嘗試讀取的附件已被移除。

  [% ELSIF error == "bug_access_denied" %]
    [% title = "沒有瀏覽權限" %]
    [% admindocslinks = {'groups.html' => '群組安全性'} %]
    你沒有瀏覽 [% terms.bug %] #[% bug_id FILTER html %] 的權限。

  [% ELSIF error == "bug_access_query" %]
    [% title = "權限不足" %]
    [% docslinks = {'myaccount.html' => '建立新帳號'} %]
    你並未授權瀏覽 [% terms.bug %] #[% bug_id FILTER html %] 。
    要瀏覽這個 [% terms.bug %] ，你必須先以擁有適當權限的帳號<a
    href="show_bug.cgi?id=
    [% bug_id FILTER url_quote %]&amp;GoAheadAndLogIn=1">登入</a>。

  [% ELSIF error == "bug_url_invalid" %]
    [% title = "無效的 $terms.Bug 回報資訊網址" %]
    <code>[% url FILTER html %]</code> 對一個 [% terms.bug %] 而言並不是有效的網址。
    [% IF reason == 'http' %]
      網址必須以 "http" 或 "https" 開頭。
    [% ELSIF reason == 'show_bug' %]
      [%+ terms.Bug %] 回報資訊網址應該指到
      已安裝的  [% terms.Bugzilla %] 中的 <code>show_bug.cgi</code> 。
    [% ELSIF reason == 'id' %]
      在網址中沒有有效的 [% terms.bug %] 編號。
    [% END %]

  [% ELSIF error == "bug_url_too_long" %]
    [% title = "無效的 $terms.Bug 回報資訊網址" %]
    [% terms.Bug %] 回報資訊網址不能長於
    [%+ constants.MAX_BUG_URL_LENGTH FILTER none %] 字元。
    <code>[% url FILTER html %]</code> 太長了。

  [% ELSIF error == "buglist_parameters_required" %]
    [% title = "需要輸入字串" %]
    [% docslinks = {'query.html' => "搜尋 $terms.bugs",
                    'query.html#list' => "$terms.Bug 清單"} %]
    在未輸入搜尋字串時，你無法搜尋，或儲存搜尋結果。

  [% ELSIF error == "chart_too_large" %]
    [% title = "圖表過大" %]
    很抱歉，但圖表的最大尺寸為 2000 x 2000 。

  [% ELSIF error == "comment_id_invalid" %]
    [% id FILTER html %] 並非有效的意見編號。

  [% ELSIF error == "comment_invalid_isprivate" %]
    你試著修改意見編號 [% id FILTER html %] 的公開與否，但它並非這個
    [% terms.bug %] 的有效意見。

  [% ELSIF error == "comment_is_private" %]
    意見編號 [% id FILTER html %] 不公開。

  [% ELSIF error == "comment_required" %]
    [% title = "需要意見" %]
    [% IF old && new %]
      當把一個 [% terms.bug %] 的狀態從
      [% old.name FILTER html %] 變更為 [% new.name FILTER html %]
      時，你必須提供<b>意見</b>。
    [% ELSIF new %]
      你必須提供這個 [% terms.bug %] 的描述。
    [% ELSE %]
      你必須對此改變提供<b>意見</b>。
    [% END %]

  [% ELSIF error == "comment_too_long" %]
    [% title = "意見過長" %]
    意見不能超過 65,535 字元。

  [% ELSIF error == "auth_classification_not_enabled" %]
    [% title = "分類功能未啟用" %]
    很抱歉，分類功能並未啟用。

  [% ELSIF error == "classification_name_too_long" %]
    [% title = "分類名稱過長" %]
    分類的名稱長度限制為 [% constants.MAX_CLASSIFICATION_SIZE FILTER html %]
    字元。 '[% name FILTER html %]' 太長了（ [% name.length %] 字元）。

  [% ELSIF error == "classification_not_specified" %]
    [% title = "你必須輸入一個分類名稱。" %]
    你必須輸入一個分類名稱。

  [% ELSIF error == "classification_already_exists" %]
    [% title = "分類已存在" %]
    分類 '[% name FILTER html %]' 已存在。

  [% ELSIF error == "classification_invalid_sortkey" %]
    [% title = "分類的排序鍵無效" %]
    排序鍵 '[% sortkey FILTER html %]'  無效。它必須在 0 和
    [% constants.MAX_SMALLINT FILTER html %] 之間。

  [% ELSIF error == "classification_not_deletable" %]
    [% title = "不允許刪除預設分類" %]
    你不能刪除預設分類。

  [% ELSIF error == "classification_has_products" %]
     很抱歉，分類中還有產品。在刪除分類之前，你必須將產品重新分配到其他的分類。

  [% ELSIF error == "component_already_exists" %]
    [% title = "子產品已存在" %]
    產品 <em>[% product.name FILTER html %]</em>
    已擁有名稱為 '[% name FILTER html %]' 的子產品。

  [% ELSIF error == "component_blank_description" %]
    [% title = "不允許子產品描述空白" %]
    你必須輸入此項子產品的描述。

  [% ELSIF error == "component_blank_name" %]
    [% title = "不允許子產品名稱空白" %]
    你必須輸入子產品的名稱。

  [% ELSIF error == "component_has_bugs" %]
    [% title = BLOCK %]子產品已有 [% terms.Bugs %][% END %]
    此子產品內已經有 [% nb FILTER html %] 個 [% terms.bugs %] ！
    在刪除子產品之前，你必須將 [% terms.bugs %] 重新分配到其他的子產品。

  [% ELSIF error == "component_name_too_long" %]
    [% title = "子產品名稱過長" %]
    子產品名稱長度限制為 [% constants.MAX_COMPONENT_SIZE FILTER html %]
    字元。 '[% name FILTER html %]' 太長了（ [% name.length %] 字元）。

  [% ELSIF error == "component_need_initialowner" %]
    [% title = "子產品必須有預設負責人" %]
    此子產品必須要有預設負責人。

  [% ELSIF error == "customfield_nonexistent" %]
    [% title = "未知的自訂欄位" %]
    並沒有名稱是 '[% name FILTER html %]' 的自訂欄位。

  [% ELSIF error == "customfield_has_activity" %]
    [% title = "自訂欄位已有活動" %]
    無法刪除自訂欄位 '[% name FILTER html %]' ，因為已有活動記錄。

  [% ELSIF error == "customfield_has_contents" %]
    [% title = "自訂欄位已有內容" %]
    無法刪除自訂欄位 '[% name FILTER html %]' ，因為至少有一個
    [% terms.bug %] 在此自訂欄位擁有非空白的值。

  [% ELSIF error == "customfield_not_obsolete" %]
    [% title = "自訂欄位並非廢棄" %]
    自訂欄位 '[% name FILTER html %]' 並非廢棄。請在嘗試刪除自訂欄位之前，先廢棄它。

  [% ELSIF error == "dependency_loop_multi" %]
    [% title = "偵測到依賴性迴圈" %]
    如果做出這些變更，下列的 [% terms.bug %] 將會同時出現在依賴關係樹的
    「會擋掉這些 [% terms.bug %] 」和「要先修好這些 [% terms.bug %] 」：
    [% FOREACH dep = deps %]
      [%+ dep FILTER bug_link(dep) FILTER none %]
    [% END %]
    <br>這會造成無限迴圈，因而不被允許。

  [% ELSIF error == "dependency_loop_single" %]
    [% title = "偵測到依賴性迴圈" %]
    你不能把一個 [% terms.bug %] 標記為「如果沒修好這個 [% terms.bug %] ，會擋掉它自己」。

  [% ELSIF error == "dupe_id_required" %]
    [% title = "需要指定重複的 $terms.Bug 編號" %]
    你必須指定和此 [% terms.bug %] 重複的 [% terms.bug %] 編號。

  [% ELSIF error == "dupe_not_allowed" %]
    [% title = "無法將 $terms.bugs 標記為重複" %]
    當你一次變更數個 [% terms.bugs %] 時，無法將 [% terms.bugs %] 標記為重複。

  [% ELSIF error == "dupe_loop_detected" %]
    [% title = "偵測到重複的迴圈" %]
    你無法將 [% terms.bug %] [% bug_id FILTER html %] 標記為和
    [% IF dupe_of == bug_id %]它自己重複。
    [% ELSE %]
      [% terms.bug %] [% dupe_of FILTER html %]重複，因為會造成重複的迴圈。
    [% END %]

  [% ELSIF error == "email_change_in_progress" %]
    [% title = "變更電子郵件進行中" %]
    變更電子郵件進行中，請檢查你的電子郵件信箱。

  [% ELSIF error == "email_confirmation_failed" %]
    [% title = "電子郵件確認失敗" %]
    電子郵件確認失敗。

  [% ELSIF error == "email_no_text_plain" %]
    你的電子郵件並未包含任何文字。 [% terms.Bugzilla %] 不允許只有
    HTML 的電子郵件，也不允許夾帶附件而沒有內容的 HTML 電子郵件。

  [% ELSIF error == "empty_group_description" %]
    [% title = "不允許群組描述空白" %]
    你必須輸入群組描述。

  [% ELSIF error == "empty_group_name" %]
    [% title = "不允許群組名稱空白" %]
    你必須輸入群組名稱。

  [% ELSIF error == "entry_access_denied" %]
    [% title = "權限不足" %]
    [% admindocslinks = {'groups.html' => '群組安全性'} %]
    很抱歉，產品 <em>[% product FILTER html %]</em> 不存在，或者你沒有權限在此產品輸入 [% terms.bug %] 。

  [% ELSIF error == "extension_create_no_name" %]
    因此程式需要，你必須替你的附加元件命名，以做為參數。

  [% ELSIF error == "extension_first_letter_caps" %]
    你的附加元件名稱的第一個字母必須為大寫。
    （你指定的是 '[% name FILTER html %]' 。）

  [% ELSIF error == "field_already_exists" %]
    [% title = "欄位已存在" %]
    欄位 '[% field.name FILTER html %]'
    ([% field.description FILTER html %]) 已存在。請選擇另一個名稱。

  [% ELSIF error == "field_cant_control_self" %]
    [% title = "欄位無法控制它自己" %]
    欄位 [% field.description FILTER html %] 無法被設定為控制它自己。

  [% ELSIF error == "field_control_must_be_select" %]
    [% title = "選擇了無效的欄位種類" %]
    只有下拉式選單和多選式選單的欄位可以被用來控制其他欄位的可見性／數值。
    [% field.description FILTER html %] 不是正確的欄位種類。

  [% ELSIF error == "field_invalid_name" %]
    [% title = "無效的欄位名稱" %]
    '[% name FILTER html %]' 並非有效的欄位名稱。
    欄位名稱只能有字母、數字，以及底線符號 (_) 。

  [% ELSIF error == "field_invalid_sortkey" %]
    [% title = "無效的欄位排序鍵" %]
    你所指定的欄位排序鍵 [% sortkey FILTER html %] 並非有效的正整數。

  [% ELSIF error == "field_missing_description" %]
    [% title = "缺少欄位描述" %]
    你必須輸入欄位描述。

  [% ELSIF error == "field_missing_name" %]
    [% title = "缺少欄位名稱" %]
    你必須輸入欄位名稱。

  [% ELSIF error == "field_value_control_select_only" %]
    [% title = "無效的控制數值欄位" %]
    只有下拉式選單和多選式選單的欄位可以有控制數值的欄位。

  [% ELSIF error == "fieldname_invalid" %]
    [% title = "指定的欄位不存在" %]
    欄位 '[% field.name FILTER html %]' 不存在，或者無法以此介面編輯。

  [% ELSIF error == "fieldvalue_already_exists" %]
    [% title = "欄位值已存在" %]
    '[% field.description FILTER html %]' 欄位已存在數值 '[% value FILTER html %]' 。

  [% ELSIF error == "fieldvalue_is_controller" %]
    [% title = "數值控制其他欄位中" %]
    你無法刪除 [% value.field.description FILTER html %] 欄位的
    '[% value.name FILTER html %]' 數值，因為
    [%- IF fields.size %]
      它控制了下列欄位的可見性：
      [%+ fields.join(', ') FILTER html %] 。
    [% END %]
    [%- IF vals.size %]
      它[% '也' IF fields.size AND vals.size -%]
      控制了下列欄位值的可見性：
      <ul>
        [% FOREACH field_name = vals.keys %]
          [% FOREACH val = vals.${field_name} %]
            <li>[% val.field.name FILTER html %] ：
              '[% val.name FILTER html %]'</li>
          [% END %]
        [% END %]
      </ul>
    [% END %]

